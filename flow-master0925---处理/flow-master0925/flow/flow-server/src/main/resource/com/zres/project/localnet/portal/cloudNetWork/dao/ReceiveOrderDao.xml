    <insert id="insertSrvAttrInfo">
        INSERT INTO GOM_BDW_SRV_ORD_ATTR_INFO ï¼ˆ
            ATTR_INFO_ID,
            SRV_ORD_ID,
            ATTR_ACTION,
            ATTR_CODE,
            ATTR_NAME,
            ATTR_VALUE,
            OLD_ATTR_VALUE,
            SOURSE,
            CREATE_DATE
        )
        SELECT SEQ_GOM_BDW_SRV_ORD_ATTR_INFO.NEXTVAL,d.* FROM (
        <foreach collection="params" item="param" index="index" separator="union all">
            SELECT #{param.srvOrdId},
            #{param.attrAction},
            #{param.attrCode},
            #{param.attrName},
            #{param.attrValue},
            #{param.oldAttrValue},
            'jike',
            sysdate
            FROM DUAL
        </foreach>) d
